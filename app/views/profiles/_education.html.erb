<div class="education">
   <% f.label_class = 'label_margin' %>
   <%= f.labelled_text_field(t('profile.from_year'),:education_from_year ,:wrap_class => cycle(:row_light2, :row_dark2)) %>
   <%= f.labelled_text_field(t('profile.to_year'),:education_to_year, :wrap_class => cycle(:row_light2, :row_dark2)) %>
   <%= f.labelled_text_field(t('profile.university'),:university, :wrap_class => cycle(:row_light2, :row_dark2)) %>
   <div class="<%= cycle(:row_light2, :row_dark2)%>">
     <%= f.remove_link theme_image("remove.png")%>
   </div>
</div>

