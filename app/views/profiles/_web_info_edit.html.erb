<% rounded_med_corner(:id => "address_details", :title => t('profile.web_info')) do %>
  <%= p.labelled_text_field(t('profile.website'), :website, :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field(t('profile.blog'), :blog, :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field(t('profile.youtube_username'),:youtube_username,:class => "youtube_icon input_text_with_icon",:wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field(t('profile.flickr_username'), :flickr_username,:class => "flickr_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field(t('profile.linkedin_name'), :linkedin_name,:class => "linkedin_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field(t('profile.twitter_username'), :twitter_username,:class => "twitter_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('AIM', :aim_name,:class => "aim_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('MSN', :msn_username,:class => "msn_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field(t('profile.yahoo_username'), :yahoo_name,:class => "yahoo_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('GTalk', :gtalk_name,:class => "gtalk_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('Skype', :skype_name,:class => "skype_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
  <%= p.labelled_text_field('Del.icio.us', :delicious_name,:class => "delicious_icon input_text_with_icon", :wrap_class => cycle(:row_light2, :row_dark2)) %>
<% end %>
