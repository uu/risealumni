<%
#locals
poll ||= feed_item.item
%>

<div id="<%= feed_item.dom_id %>">
  <div class="comments">
    <img src="/images/poll_icon.png" title="<%= t('feed_item.user_photo_icon') %>"  alt="<%= t('feed_item.user_photo_icon') %>" /> 
    <strong><%= link_to h(poll.profile.full_name), poll.profile %> <%= t('feed_item.user_created_poll') %> <%= time_ago_in_words poll.created_at %> <%= t('ago') %></strong><br />
    <%= link_to poll.question, profile_poll_path(poll.profile, poll) %>
  </div>
    <%= x_feed_link(feed_item) if me %>    
</div>
